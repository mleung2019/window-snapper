{
  "snap-full": {
    "type": "keybinding",
    "default": ["<Super><Alt>Up"],
    "description": "Snap window to full screen"
  },
  "snap-left": {
    "type": "keybinding",
    "default": ["<Super><Alt>Left"],
    "description": "Snap window to left half"
  },
  "snap-right": {
    "type": "keybinding",
    "default": ["<Super><Alt>Right"],
    "description": "Snap window to right half"
  },
  "snap-top-left": {
    "type": "keybinding",
    "default": ["<Super><Alt>u"],
    "description": "Snap window to top left quarter"
  },
  "snap-top-right": {
    "type": "keybinding",
    "default": ["<Super><Alt>i"],
    "description": "Snap window to top right quarter"
  },
  "snap-bottom-left": {
    "type": "keybinding",
    "default": ["<Super><Alt>j"],
    "description": "Snap window to bottom left quarter"
  },
  "snap-bottom-right": {
    "type": "keybinding",
    "default": ["<Super><Alt>k"],
    "description": "Snap window to bottom right quarter"
  }
}
